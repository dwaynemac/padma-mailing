<%= form_for template do |f| %>
  <input type="hidden" name="back_to" value="<%= url_for params %>" ></input>
  <% folders = if @current_folder.nil?
                []
    elsif @current_folder.parent.nil?
      [["..",nil]]
    else
      [[@current_folder.parent.name , @current_folder.parent.id]]
    end %>
  <%= f.select :parent_templates_folder_id, options_for_select(folders+@folders.map{|f| [f.name,f.id]}) %>
  <%= f.submit t(".move") %>
<% end %>
